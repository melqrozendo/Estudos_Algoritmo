algoritmo "cambio"
var
     c,Q: inteiro
     R,D: Real
inicio
      c <- 1
      Escreva("Digite quantas vezes quer converter:")
      Leia(Q)
      Enquanto c <= Q faca
               Escreva("Digite o valor em R$: ")
               Leia(R)
               D<-R/5,24
               Escreval("O valor convertido em US$: ", D)
               c <- c + 1
      FimEnquanto
fimalgoritmo
