algoritmo "Alfanumerico"
var
     N:inteiro
inicio
      Escreva("Digite um numero:")
      Leia(N)
      Escolha N
              caso 1
              Escreva("O numero ",N," corresponde a Letra A")
              caso 2
              Escreva("B")
              caso 3
              Escreva("C")
      FimEscolha
fimalgoritmo
